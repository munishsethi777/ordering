<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Ordering System</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    @Styles.Render("~/Content/themes/base/css")
    <!-- Le styles -->
    <link rel="stylesheet" href="../../Content/css/bootstrap.min.css">
    <link rel="stylesheet" href="../../Content/custom-theme/jquery-ui-1.10.2.custom.css">
    <link rel="stylesheet" href="../../Content/css/font-awesome.min.css">
    <!-- jQuery + jqGrid -->
    <link rel="stylesheet" href="../../Content/ui.jqgrid.css" />
    <link rel="stylesheet" href="../../Content/jqGrid.overrides.css">
    
    <!-- bootstrap select -->
    <link href="../../Content/css/bootstrap-select.css" rel="stylesheet" type="text/css" />
    <!-- progress bar -->
    <link href="../../Content/css/progressBar2.css" rel="stylesheet" type="text/css" />
    <!--[if IE 7]>
    <link rel="stylesheet" href="assets/css/font-awesome-ie7.min.css">
    <![endif]-->
    <!--[if lt IE 9]>
            <link rel="stylesheet" href="css/custom-theme/jquery.ui.1.10.2.ie.css">
            <![endif]-->

    <!-- excel - edit -->
    <link href="../../Content/jquery.handsontable.full.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/jquery.handsontable.removeRow.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="../../Content/css/docs.css" type="text/css">
    <link href="../../Content/css/mvcef.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="../../Content/css/prettify.css" type="text/css" />
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
            <![endif]-->
    <!-- Le fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../../Content/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../../Content/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../../Content/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="../../Content/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="../../Content/ico/favicon.png">
</head>
<body id="bodySectionProgress" data-spy="scroll" data-target=".bs-docs-sidebar" data-twttr-rendered="true"
    class="bodybackgroundcolor">
    <!-- Navbar - Horizontal Menu ================================================== -->
    @{ Html.RenderAction("HorizontalMenu", "NavMenu", new { area = "" }); }
    <!-- Body - Vertical Nav bar ================================================== -->
    <div class="">
        <div class="row-fluid">
            <div class="span12" style="float: right;">
                <div id="alertSuccess" style="display: none;">
                    <div class="alert alert-success">
                        <button type="button" class="close" data-dismiss="alert">
                            &times;</button>
                        <strong>Well done!</strong> You successfully read this important alert message.
                    </div>
                </div>
                <section id="bodySection">
                    @RenderBody()
                </section>
                <div id="alertError" style="display: none;">
                    <div class="alert alert-error">
                        <button type="button" class="close" data-dismiss="alert">
                            &times;</button>
                        <span class="errorMsg">Change a few things up and try submitting again.</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--hidden control -->
    <input type="hidden" name="hdnCustomerId" id="hdnCustomerId" />
    <input type="hidden" name="hdnCustomerName" id="hdnCustomerName" />
    <input type="hidden" name="hdnUserId" id="hdnUserId" />
    <input type="hidden" name="hdnLoginRoleId" id="hdnLoginRoleId" />
    <input type="hidden" name="hdnSelectedCustomerId" id="hdnSelectedCustomerId" />
    <input type="hidden" name="hdnOrderId" id="hdnOrderId" value="" />
    <!--for all pages-->
    <div id="dialog-add" style="display: none">
    </div>
    <!-- Confirmation Dialog Box -->
    <div id="dialog-confirm" title="Confirmation" style="display: none;">
        <p class="confirmmsg">
            <span class="ui-icon ui-icon-alert" style="float: left; margin: 0 7px 20px 0;"></span>
        </p>
    </div>
    <!-- Alert Message -->
    <div id="dialog-message" title="Alert" style="display: none;">
        <p class="confirmmsg">
            <span class="ui-icon ui-icon-circle-check" style="float: left; margin: 0 7px 50px 0;">
            </span>
        </p>
    </div>
    <!-- Session Timeout Message-->
    <div id="sessionTimeoutWarning" style="display: none;">
    </div>
    <!-- To generate the Lookup footer -->
    <div id="lookupFooter" style="display: none;">
    </div>
    <!-- To generate the Lookup header -->
    <div id="lookupHeader" style="display: none;">
    </div>
    <!-- Footer ================================================== -->
    <footer id="footerWithProgressBar" class="footer navbar-fixed-bottom">
        <div class="container">
        <div class="row">
            <div class="span5">
            </div>
            <div class="span5">
                <span> &copy Copyright 2014. All Rights Reserved</span>
            </div>
            <div class="span5">
            </div>
          </div>
        </div>
    </footer>
    <!-- Le javascript ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    @*@Scripts.Render("~/bundles/jquery")*@ @*@Scripts.Render("~/bundles/jqueryui")*@
    <script src="../../Scripts/jquery-1.9.1.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery-migrate-1.2.1.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.validate.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.validate.unobtrusive.js" type="text/javascript"></script>

    <script src="../../Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="../../Scripts/bootstrap-button.js" type="text/javascript"></script>
    <!--JSON-->
    <script src="../../Scripts/json2.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.xml2json.js" type="text/javascript"></script>
    <!-- jQuery + jqGrid -->
    <script src="../../Scripts/grid.locale-en.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.jqGrid.js" type="text/javascript"></script>
    <script src="../../Scripts/holder.js" type="text/javascript"></script>
    @*<script src="../../Scripts/jquery-ui-1.10.2.custom.min.js" type="text/javascript"></script>*@
    <script src="../../Scripts/jquery-ui-1.10.2.js" type="text/javascript"></script>
    @*<script src="../../Scripts/jquery-1.9.1-vsdoc.js" type="text/javascript"></script>*@
    <script src="../../Scripts/prettify.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.maskedinput-1.2.2.js" type="text/javascript"></script>
    <!-- time picker addon-->
    <script src="../../Scripts/jQueryTimepickerAddon.js" type="text/javascript"></script>

    <!-- bootstrap select -->
    <script src="../../Scripts/bootstrap-select.js" type="text/javascript"></script>
    @*<script src="../../Scripts/bootstrap-button.js" type="text/javascript"></script>*@
    <!-- progress bar -->
    <script src="../../Scripts/progressBar2.js" type="text/javascript"></script>
    <!--timer functionality scripts-->
    <script src="../../Scripts/jquery.timer.js" type="text/javascript"></script>
    <!--Session timeout scripts-->
    @*
    <script src="../../Scripts/jquery.idletimer.js" type="text/javascript"></script>
    <script src="../../Scripts/session.js" type="text/javascript"></script>*@
    <!-- Lookup control -->
    <script src="../../Scripts/lookup.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.mcautocomplete.js" type="text/javascript"></script>
    <!--manual pagination-->
    <script src="../../Scripts/pagination.js" type="text/javascript"></script>
    
    <!-- excel - edit -->
    <script src="../../Scripts/jquery.handsontable.full.js" type="text/javascript"></script>
    <script src="../../Scripts/jquery.handsontable.removeRow.js" type="text/javascript"></script>

    <!-- jQuery general framework components-->
    <script src="../../Scripts/mvc-ef.js" type="text/javascript"></script>
    @*@Scripts.Render("~/bundles/jqueryval")*@
    <script type="text/javascript" language="javascript">
        $('#hdnCustomerId').val("0");

        $(document).ready(function () {

            if ('@Session["CustomerId"]' != null) {
                $('#hdnCustomerName').val('@Session["CustomerName"].ToString()');
                $('#hdnCustomerId').val('@Session["CustomerId"].ToString()');
                $('#hdnUserId').val('@Session["UserId"].ToString()');
                $('#hdnLoginRoleId').val('@Session["RoleId"].ToString()');
                $('#hdnSelectedCustomerId').val('@Session["CustomerId"].ToString()');
            }
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
